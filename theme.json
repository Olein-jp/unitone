{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/theme.json",
	"version": 2,
	"settings": {
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"color": {
			"link": true,
			"gradients": [
				{
					"slug":"unitone-accent",
					"gradient":"linear-gradient(135deg,hsl(276,49%,47%) 0%,hsl(253,49%,17%) 100%)",
					"name":"Accent color gradation"
				}
			],
			"palette": [
				{
					"slug": "unitone/accent",
					"color": "#1f1640",
					"name": "Accent color"
				},
				{
					"slug": "unitone/bright-gray",
					"color": "#f8f9fa",
					"name": "Bright gray"
				},
				{
					"slug": "unitone/light-gray",
					"color": "#e9ecef",
					"name": "Light gray"
				},
				{
					"slug": "unitone/gray",
					"color": "#ced4da",
					"name": "gray"
				},
				{
					"slug": "unitone/dark-gray",
					"color": "#868e96",
					"name": "Dark gray"
				}
			]
		},
		"custom": {
			"container-max-width": "min(100% - var(--unitone--rem1) * 2, var(--unitone--container-max-width))",
			"content-max-width": "min(100% - var(--unitone--rem1) * 2, var(--unitone--measure))"
		},
		"layout": {
			"contentSize": "var(--wp--custom--content-max-width)",
			"wideSize": "var(--wp--custom--container-max-width)"
		},
		"typography": {
			"dropCap": false,
			"customFontSize": false,
			"textTransform": true,
			"lineHeight": true,
			"fontSizes": [
				{
					"slug": "unitone-xs",
					"size": "0.8rem",
					"name": "XS"
				},
				{
					"slug": "unitone-s",
					"size": "0.88rem",
					"name": "S"
				},
				{
					"slug": "unitone-m",
					"size": "1rem",
					"name": "M"
				},
				{
					"slug": "unitone-l",
					"size": "1.14rem",
					"name": "L"
				},
				{
					"slug": "unitone-xl",
					"size": "1.33rem",
					"name": "XL"
				},
				{
					"slug": "unitone-2xl",
					"size": "1.6rem",
					"name": "2XL"
				},
				{
					"slug": "unitone-3xl",
					"size": "2rem",
