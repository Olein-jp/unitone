				},
				{
					"slug": "unitone-4xl",
					"size": "2.66rem",
					"name": "4XL"
				},
				{
					"slug": "unitone-5xl",
					"size": "4rem",
					"name": "5XL"
				},
				{
					"slug": "unitone-6xl",
					"size": "8rem",
					"name": "6XL"
				}
			]
		},
		"spacing": {
			"blockGap": true,
			"padding": false
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "var(--unitone--s1)"
		}
